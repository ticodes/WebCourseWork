{
  "$schema": "https://json-schema.org/draft-01/schema#",
  "type": "object",
  "properties": {
    "page" : {
      "type" : "number"
    },
    "total":{
      "type": "number"
    },
    "total_pages": {
      "type": "number"
    },
    "data": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "number"
          },
          "email": {
            "type": "string"
          },
          "first_name": {
            "type": "string"
          },
          "last_name": {
            "type": "string"
          },
          "avatar": {
            "type": "string"
          }
        },
        "required": ["id", "email", "first_name", "last_name", "avatar"]
      }
    },
    "support": {
      "type": "object",
      "properties": {
        "url": {
          "type": "string"
        },
        "text": {
          "type": "string"
        }
      },
      "required": ["url", "text"]
    }
  },
  "required": ["page", "total", "total_pages", "data", "support"]
}